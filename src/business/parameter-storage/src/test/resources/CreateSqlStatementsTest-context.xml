<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context 
       http://www.springframework.org/schema/context/spring-context-2.5.xsd
       http://camel.apache.org/schema/spring 
       http://camel.apache.org/schema/spring/camel-spring.xsd
       ">

	<context:annotation-config />

	<bean id="createSqlStatement" class="org.hbird.business.parameterstorage.archiver.CreateSqlStatement" />

	<camelContext id="createSqlStatementContext" xmlns="http://camel.apache.org/schema/spring">
		<template id="producer" />

		<route>
			<from uri="direct:Parameter" />
			<to uri="bean:createSqlStatement" />
			<to uri="mock:Result" />
		</route>

	</camelContext>
</beans>



